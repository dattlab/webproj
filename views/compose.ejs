<%- include("partials/header") %>

<h1>Compose</h1>

<form action="/" method="post">
  <div class="new-post-title-entrybox">
    <label for="newPostTitle">Title</label>
    <input type="text" name="newPostTitle" id="newPostTitle">
  </div>

  <div class="new-post-content-entrybox">
    <label for="newPostContent">Content</label>
    <textarea name="newPostContent" id="newPostContent" rows="15"></textarea>
  </div>

  <button type="submit" id="publishButton">Publish</button>
</form>

<%- include("partials/footer") %>